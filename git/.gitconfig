[core]
	editor = nvim
[credential]
    helper = store
[color]
        branch = auto
        diff = auto
        interactive = auto
        status = auto
        ui = auto
[color "status"]
        added = green
        changed = red bold
        untracked = magenta bold
[diff]
        renames = copies
[fetch]
        prune = true
[pull]
        rebase = true
[push]
        default = simple
[rebase]
        autostash = true
        who = false
